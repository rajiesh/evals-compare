# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Search API Configuration (required for MCP Search Server)
# Get your Google Custom Search credentials:
# 1. API Key: https://console.cloud.google.com/apis/credentials
# 2. Custom Search Engine ID: https://programmablesearchengine.google.com/
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_custom_search_engine_id_here

# Evaluation Framework Keys (if needed)
TRUELENS_API_KEY=your_truelens_key_here

# Application Settings
LOG_LEVEL=INFO
MAX_SEARCH_RESULTS=10
CACHE_ENABLED=true
